		<script tyle="text/javascript">
			$(document).ready(function() {
        var counter = 9;
        var interval = setInterval(function() {
          if(counter == 1) {
            $('#ss_continue').removeClass('disabled');
            $('#ss_continue').prop('disabled', false);
          }
          if(counter <= 0) {
            $('#ss_continue').trigger('click');
            clearInterval(interval);
          } else {
            $('#ss_timer').text('Continue Login in ' + counter);
          }
          counter--;
        }, 1000);
      });
		</script>

